		<script>
			const images = document.querySelectorAll('img[data-pf-type="Image"], a[data-pf-type="Image"] img')
			images.forEach(img => {
				img.removeAttribute("srcset")
				const regex = /_([0-9]){2,}x/
				const text = img.src.match(regex)
				if (text) {
					img.src = img.src.replace(text[0], '');
				}
			});
		</script>
      
